<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ApplicantTypeServlet</servlet-name>
        <servlet-class>ePanjeeyanServlet.ApplicantTypeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetHolidayServlet</servlet-name>
        <servlet-class>ePanjeeyanServlet.GetHolidayServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeedCategoryServlet</servlet-name>
        <servlet-class>ePanjeeyanServlet.DeedCategoryServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubDeedCategoryServlet</servlet-name>
        <servlet-class>ePanjeeyanServlet.SubDeedCategoryServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewStatusServlet</servlet-name>
        <servlet-class>ePanjeeyanServlet.ViewStatusServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileUploadServlet</servlet-name>
        <servlet-class>org.nic.controller.FileUploadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrationOfficeServlet</servlet-name>
        <servlet-class>ePanjeeyanServlet.RegistrationOfficeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ApplicantTypeServlet</servlet-name>
        <url-pattern>/getapplicanttype</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetHolidayServlet</servlet-name>
        <url-pattern>/getholiday</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeedCategoryServlet</servlet-name>
        <url-pattern>/getdeedcategory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubDeedCategoryServlet</servlet-name>
        <url-pattern>/getsubdeed</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewStatusServlet</servlet-name>
        <url-pattern>/viewstatusservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileUploadServlet</servlet-name>
        <url-pattern>/fileupload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrationOfficeServlet</servlet-name>
        <url-pattern>/registrationoffice</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
